{
    "name": "usmanahmedmalik/inspiring-quran",
    "description": "A modern Laravel package for displaying random Quranic Ayats - forever-proof with zero external dependencies",
    "keywords": [
        "laravel",
        "quran",
        "islamic",
        "ayat",
        "inspiration",
        "artisan",
        "laravel-package"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Usman Ahmed Malik",
            "email": "usmanahmedmalik@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Usmanahmedmalik\\InspiringQuran\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Usmanahmedmalik\\InspiringQuran\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Usmanahmedmalik\\InspiringQuran\\QuranServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
